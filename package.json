{
  "name": "carriere-web",
  "description": "Website for S.A.R.L Gilbert Alla",
  "version": "1.0.0",
  "author": "Bruno Alla <alla.brunoo@gmail.com>",
  "dependencies": {
    "@browniebroke/gatsby-image-gallery": "3.3.3",
    "gatsby": "2.23.3",
    "gatsby-image": "2.4.7",
    "gatsby-plugin-google-analytics": "2.3.4",
    "gatsby-plugin-manifest": "2.4.11",
    "gatsby-plugin-netlify": "2.3.5",
    "gatsby-plugin-offline": "3.2.9",
    "gatsby-plugin-react-helmet": "3.3.4",
    "gatsby-plugin-react-svg": "3.0.0",
    "gatsby-plugin-sass": "2.3.4",
    "gatsby-plugin-sentry": "1.0.1",
    "gatsby-plugin-sharp": "2.6.11",
    "gatsby-plugin-sitemap": "2.4.5",
    "gatsby-source-datocms": "2.2.6",
    "gatsby-source-filesystem": "2.3.11",
    "gatsby-transformer-sharp": "2.5.5",
    "node-sass": "4.14.1",
    "nuka-carousel": "4.7.0",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.1.0",
    "slugify": "1.4.0"
  },
  "devDependencies": {
    "bootstrap": "4.5.0",
    "bundlesize": "0.18.0",
    "eslint": "7.3.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.0",
    "prettier": "2.0.5",
    "textlint": "11.6.3"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "prebundlesize": "yarn run build",
    "bundlesize": "bundlesize",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.+(js|jsx|json|css|scss)\"",
    "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "lint:text": "textlint src/pages/**/index.md",
    "lint:textfix": "textlint --fix src/pages/**/index.md",
    "test": "prettier --check \"**/*.+(js|jsx|json|css|scss)\""
  },
  "bundlesize": [
    {
      "path": "./public/app-*.js",
      "maxSize": "35KB"
    },
    {
      "path": "./public/component---src-pages-404-*.js",
      "maxSize": "400B"
    },
    {
      "path": "./public/component---src-pages-contact-*.js",
      "maxSize": "700B"
    },
    {
      "path": "./public/component---src-pages-photos-*.js",
      "maxSize": "3.2KB"
    },
    {
      "path": "./public/component---src-pages-index-*.js",
      "maxSize": "24KB"
    },
    {
      "path": "./public/component---src-templates-gallery-page-*.js",
      "maxSize": "27KB"
    },
    {
      "path": "./public/styles.*.css",
      "maxSize": "13.2KB"
    },
    {
      "path": "./public/styles-*.js",
      "maxSize": "150B"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  }
}
