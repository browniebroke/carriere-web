{
  "name": "carriere-web",
  "description": "Website for S.A.R.L Gilbert Alla",
  "version": "1.0.0",
  "author": "Bruno Alla <alla.brunoo@gmail.com>",
  "dependencies": {
    "@browniebroke/gatsby-image-gallery": "6.1.2",
    "@browniebroke/react-ui-components": "1.7.2",
    "@fontsource/montserrat": "4.2.2",
    "babel-plugin-styled-components": "1.12.0",
    "gatsby": "3.2.1",
    "gatsby-plugin-google-analytics": "3.2.0",
    "gatsby-plugin-image": "1.2.0",
    "gatsby-plugin-manifest": "3.2.0",
    "gatsby-plugin-netlify": "3.2.0",
    "gatsby-plugin-offline": "4.2.0",
    "gatsby-plugin-react-helmet": "4.2.0",
    "gatsby-plugin-react-svg": "3.0.0",
    "gatsby-plugin-sentry": "1.0.1",
    "gatsby-plugin-sharp": "3.2.0",
    "gatsby-plugin-sitemap": "3.2.0",
    "gatsby-plugin-styled-components": "4.2.0",
    "gatsby-plugin-typescript": "3.2.0",
    "gatsby-plugin-typography": "3.2.0",
    "gatsby-source-datocms": "2.6.10",
    "gatsby-source-filesystem": "3.2.0",
    "gatsby-transformer-sharp": "3.2.0",
    "nuka-carousel": "4.7.7",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0",
    "react-typography": "0.16.19",
    "slugify": "1.5.0",
    "styled-components": "5.2.1",
    "typescript": "4.2.3",
    "typography": "0.16.19"
  },
  "devDependencies": {
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.3",
    "@types/react-helmet": "6.1.0",
    "@types/styled-components": "5.1.9",
    "@typescript-eslint/parser": "4.21.0",
    "bundlewatch": "0.3.2",
    "eslint": "7.23.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.23.1",
    "prettier": "2.2.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "prebundlewatch": "yarn run build",
    "bundlewatch": "bundlewatch",
    "develop": "gatsby develop",
    "format": "prettier --write .",
    "lint:eslint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:prettier": "prettier --check .",
    "lint:tsc": "tsc"
  },
  "renovate": {
    "extends": [
      "github>browniebroke/renovate-configs:js-app"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "prettier"
    ],
    "plugins": [
      "react",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "es6": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "semi": false,
          "singleQuote": true,
          "trailingComma": "es5"
        }
      ]
    }
  },
  "bundlewatch": [
    {
      "path": "./public/app-*.js",
      "maxSize": "25KB"
    },
    {
      "path": "./public/component---src-pages-404-*.js",
      "maxSize": "500B"
    },
    {
      "path": "./public/component---src-pages-contact-*.js",
      "maxSize": "1KB"
    },
    {
      "path": "./public/component---src-pages-produits-*.js",
      "maxSize": "3.5KB"
    },
    {
      "path": "./public/component---src-pages-index-*.js",
      "maxSize": "25KB"
    },
    {
      "path": "./public/component---src-templates-gallery-page-*.js",
      "maxSize": "14KB"
    },
    {
      "path": "./public/styles.*.css",
      "maxSize": "25KB"
    }
  ]
}
