    "babel-plugin-styled-components": "1.12.0",
    "gatsby": "2.32.3",
    "gatsby-image": "2.11.0",
    "gatsby-plugin-google-analytics": "2.11.0",
    "gatsby-plugin-manifest": "2.12.0",
    "gatsby-plugin-netlify": "2.11.0",
    "gatsby-plugin-offline": "3.10.0",
    "gatsby-plugin-react-helmet": "3.10.0",
    "gatsby-plugin-react-svg": "3.0.0",
    "gatsby-plugin-sentry": "1.0.1",
    "gatsby-plugin-sharp": "2.14.1",
    "gatsby-plugin-sitemap": "2.12.0",
    "gatsby-plugin-styled-components": "3.10.0",
    "gatsby-plugin-typography": "2.12.0",
    "gatsby-source-datocms": "2.5.10",
    "gatsby-source-filesystem": "2.11.0",
    "gatsby-transformer-sharp": "2.12.0",
    "nuka-carousel": "4.7.5",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0",
    "react-typography": "0.16.19",
    "slugify": "1.4.6",
    "styled-components": "5.2.1",
    "typography": "0.16.19"
  },
  "devDependencies": {
    "bundlewatch": "0.3.2",
    "eslint": "7.20.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "prettier": "2.2.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "prebundlewatch": "yarn run build",
    "bundlewatch": "bundlewatch",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.+(js|jsx|json|css|scss)\"",
    "lint:eslint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "lint:prettier": "prettier --check \"**/*.+(js|jsx|json|css|scss)\""
  },
  "bundlewatch": [
    {
      "path": "./public/app-*.js",
      "maxSize": "20KB"
    },
    {
      "path": "./public/component---src-pages-404-*.js",
      "maxSize": "500B"
    },
    {
      "path": "./public/component---src-pages-contact-*.js",
      "maxSize": "1.5KB"
    },
    {
      "path": "./public/component---src-pages-produits-*.js",
      "maxSize": "4.1KB"
    },
    {
      "path": "./public/component---src-pages-index-*.js",
      "maxSize": "25KB"
    },
    {
      "path": "./public/component---src-templates-gallery-page-*.js",
      "maxSize": "14KB"
    },
    {
      "path": "./public/styles.*.css",
      "maxSize": "25KB"
    },
    {
      "path": "./public/styles-*.js",
      "maxSize": "130B"
    }
  ]
}
