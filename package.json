{
  "name": "carriere-web",
  "description": "Website for S.A.R.L Gilbert Alla",
  "version": "1.0.0",
  "author": "Bruno Alla <alla.brunoo@gmail.com>",
  "dependencies": {
    "@browniebroke/gatsby-image-gallery": "2.0.1",
    "gatsby": "2.18.1",
    "gatsby-image": "2.2.33",
    "gatsby-plugin-google-analytics": "2.1.28",
    "gatsby-plugin-manifest": "2.2.29",
    "gatsby-plugin-netlify": "2.1.25",
    "gatsby-plugin-offline": "3.0.22",
    "gatsby-plugin-react-helmet": "3.1.15",
    "gatsby-plugin-react-svg": "2.1.2",
    "gatsby-plugin-sass": "2.1.23",
    "gatsby-plugin-sentry": "1.0.1",
    "gatsby-plugin-sharp": "2.3.2",
    "gatsby-plugin-sitemap": "2.2.21",
    "gatsby-source-filesystem": "2.1.38",
    "gatsby-transformer-remark": "2.6.37",
    "gatsby-transformer-sharp": "2.3.5",
    "nuka-carousel": "4.5.13",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-helmet": "5.2.1"
  },
  "devDependencies": {
    "bootstrap": "4.4.0",
    "bundlesize": "0.18.0",
    "eslint": "6.6.0",
    "eslint-config-prettier": "6.7.0",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "7.16.0",
    "node-sass": "4.13.0",
    "prettier": "1.19.1",
    "textlint": "11.5.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "prebundlesize": "yarn run build",
    "bundlesize": "bundlesize",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.+(js|jsx|json|css|scss)\"",
    "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "lint:text": "textlint src/pages/**/index.md",
    "lint:textfix": "textlint --fix src/pages/**/index.md",
    "test": "prettier --check \"**/*.+(js|jsx|json|css|scss)\""
  },
  "bundlesize": [
    {
      "path": "./public/app-*.js",
      "maxSize": "40 kB"
    },
    {
      "path": "./public/component---src-pages-!(index)-*.js",
      "maxSize": "1 kB"
    },
    {
      "path": "./public/component---src-pages-index-*.js",
      "maxSize": "25 kB"
    },
    {
      "path": "./public/component---src-templates*.js",
      "maxSize": "30 kB"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  }
}
