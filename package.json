{
  "name": "carriere-web",
  "description": "Website for S.A.R.L Gilbert Alla",
  "version": "1.0.0",
  "author": "Bruno Alla <alla.brunoo@gmail.com>",
  "dependencies": {
    "@browniebroke/gatsby-image-gallery": "7.0.7",
    "@browniebroke/react-ui-components": "2.0.0",
    "@fontsource/montserrat": "4.5.13",
    "babel-plugin-styled-components": "2.0.7",
    "gatsby": "4.24.3",
    "gatsby-plugin-google-gtag": "4.24.0",
    "gatsby-plugin-image": "2.24.0",
    "gatsby-plugin-manifest": "4.24.0",
    "gatsby-plugin-netlify": "5.0.1",
    "gatsby-plugin-offline": "5.24.0",
    "gatsby-plugin-react-helmet": "5.24.0",
    "gatsby-plugin-react-svg": "3.1.0",
    "gatsby-plugin-sentry": "1.0.1",
    "gatsby-plugin-sharp": "4.24.0",
    "gatsby-plugin-sitemap": "5.24.0",
    "gatsby-plugin-styled-components": "5.24.0",
    "gatsby-plugin-typescript": "4.24.0",
    "gatsby-source-datocms": "4.0.4",
    "gatsby-source-filesystem": "4.24.0",
    "gatsby-transformer-sharp": "4.24.0",
    "nuka-carousel": "5.4.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet": "6.1.0",
    "sanitize.css": "13.0.0",
    "slugify": "1.6.5",
    "styled-components": "5.3.6",
    "typescript": "4.8.4"
  },
  "devDependencies": {
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@types/react-helmet": "6.1.5",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/parser": "5.39.0",
    "bundlewatch": "0.3.3",
    "eslint": "8.25.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.9",
    "prettier": "2.7.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "serve": "gatsby serve",
    "bundlewatch": "bundlewatch",
    "develop": "gatsby develop",
    "format": "prettier --write .",
    "lint:eslint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:prettier": "prettier --check .",
    "lint:tsc": "tsc"
  },
  "renovate": {
    "extends": [
      "github>browniebroke/renovate-configs:js-app"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "prettier"
    ],
    "plugins": [
      "react",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "es6": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "semi": false,
          "singleQuote": true,
          "trailingComma": "es5"
        }
      ]
    }
  },
  "bundlewatch": [
    {
      "path": "./public/app-*.js",
      "maxSize": "40KB"
    },
    {
      "path": "./public/component---src-pages-404-*.js",
      "maxSize": "500B"
    },
    {
      "path": "./public/component---src-pages-contact-*.js",
      "maxSize": "1KB"
    },
    {
      "path": "./public/component---src-pages-produits-*.js",
      "maxSize": "5KB"
    },
    {
      "path": "./public/component---src-pages-index-*.js",
      "maxSize": "25KB"
    },
    {
      "path": "./public/component---src-templates-gallery-page-*.js",
      "maxSize": "25KB"
    },
    {
      "path": "./public/styles.*.css",
      "maxSize": "70KB"
    }
  ]
}
