{
  "name": "carriere-web",
  "description": "Website for S.A.R.L Gilbert Alla",
  "version": "1.0.0",
  "author": "Bruno Alla <alla.brunoo@gmail.com>",
  "dependencies": {
    "@chakra-ui/gatsby-plugin": "3.1.3",
    "@chakra-ui/react": "2.8.2",
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@fontsource/montserrat": "5.0.16",
    "framer-motion": "10.16.16",
    "gatsby": "5.13.0",
    "gatsby-plugin-google-gtag": "5.13.0",
    "gatsby-plugin-image": "3.13.0",
    "gatsby-plugin-manifest": "5.13.0",
    "gatsby-plugin-netlify": "5.1.1",
    "gatsby-plugin-offline": "6.13.0",
    "gatsby-plugin-react-helmet": "6.13.0",
    "gatsby-plugin-react-svg": "3.3.0",
    "gatsby-plugin-sentry": "1.0.1",
    "gatsby-plugin-sharp": "5.13.0",
    "gatsby-plugin-sitemap": "6.13.0",
    "gatsby-plugin-typescript": "5.13.0",
    "gatsby-source-datocms": "5.1.2",
    "gatsby-source-filesystem": "5.13.0",
    "gatsby-transformer-sharp": "5.13.0",
    "nuka-carousel": "7.0.0",
    "photoswipe": "5.4.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet": "6.1.0",
    "react-photoswipe-gallery": "3.0.1",
    "slugify": "1.6.6",
    "typescript": "5.3.3"
  },
  "devDependencies": {
    "@types/react": "18.2.45",
    "@types/react-dom": "18.2.18",
    "@types/react-helmet": "6.1.11",
    "@typescript-eslint/parser": "6.15.0",
    "bundlewatch": "0.3.3",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.0",
    "eslint-plugin-react": "7.33.2",
    "prettier": "3.1.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "serve": "gatsby serve",
    "bundlewatch": "bundlewatch",
    "develop": "gatsby develop",
    "format": "prettier --write .",
    "lint:eslint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:prettier": "prettier --check .",
    "lint:tsc": "tsc"
  },
  "renovate": {
    "extends": [
      "github>browniebroke/renovate-configs:js-app"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "prettier"
    ],
    "plugins": [
      "react",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "es6": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "semi": false,
          "singleQuote": true,
          "trailingComma": "es5"
        }
      ]
    }
  },
  "bundlewatch": [
    {
      "path": "./public/app-*.js",
      "maxSize": "150KB"
    },
    {
      "path": "./public/component---src-pages-404-*.js",
      "maxSize": "500B"
    },
    {
      "path": "./public/component---src-pages-contact-*.js",
      "maxSize": "1KB"
    },
    {
      "path": "./public/component---src-pages-produits-*.js",
      "maxSize": "10KB"
    },
    {
      "path": "./public/component---src-pages-index-*.js",
      "maxSize": "25KB"
    },
    {
      "path": "./public/component---src-templates-gallery-page-*.js",
      "maxSize": "5KB"
    },
    {
      "path": "./public/styles.*.css",
      "maxSize": "100KB"
    }
  ]
}
