{
  "name": "carriere-web",
  "description": "Website for S.A.R.L Gilbert Alla",
  "version": "1.0.0",
  "author": "Bruno Alla <alla.brunoo@gmail.com>",
  "dependencies": {
    "@browniebroke/gatsby-image-gallery": "7.2.1",
    "@browniebroke/react-ui-components": "2.2.0",
    "@fontsource/montserrat": "4.5.14",
    "babel-plugin-styled-components": "2.0.7",
    "gatsby": "5.8.0",
    "gatsby-plugin-google-gtag": "5.8.0",
    "gatsby-plugin-image": "3.8.0",
    "gatsby-plugin-manifest": "5.8.0",
    "gatsby-plugin-netlify": "5.1.0",
    "gatsby-plugin-offline": "6.8.0",
    "gatsby-plugin-react-helmet": "6.8.0",
    "gatsby-plugin-react-svg": "3.3.0",
    "gatsby-plugin-sentry": "1.0.1",
    "gatsby-plugin-sharp": "5.8.0",
    "gatsby-plugin-sitemap": "6.8.0",
    "gatsby-plugin-styled-components": "6.8.0",
    "gatsby-plugin-typescript": "5.8.0",
    "gatsby-source-datocms": "5.0.4",
    "gatsby-source-filesystem": "5.8.0",
    "gatsby-transformer-sharp": "5.8.0",
    "nuka-carousel": "5.5.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet": "6.1.0",
    "sanitize.css": "13.0.0",
    "slugify": "1.6.6",
    "styled-components": "5.3.9",
    "typescript": "5.0.2"
  },
  "devDependencies": {
    "@types/react": "18.0.30",
    "@types/react-dom": "18.0.11",
    "@types/react-helmet": "6.1.6",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/parser": "5.57.0",
    "bundlewatch": "0.3.3",
    "eslint": "8.36.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "prettier": "2.8.7"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "serve": "gatsby serve",
    "bundlewatch": "bundlewatch",
    "develop": "gatsby develop",
    "format": "prettier --write .",
    "lint:eslint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:prettier": "prettier --check .",
    "lint:tsc": "tsc"
  },
  "renovate": {
    "extends": [
      "github>browniebroke/renovate-configs:js-app"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "prettier"
    ],
    "plugins": [
      "react",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "es6": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "semi": false,
          "singleQuote": true,
          "trailingComma": "es5"
        }
      ]
    }
  },
  "bundlewatch": [
    {
      "path": "./public/app-*.js",
      "maxSize": "40KB"
    },
    {
      "path": "./public/component---src-pages-404-*.js",
      "maxSize": "500B"
    },
    {
      "path": "./public/component---src-pages-contact-*.js",
      "maxSize": "1KB"
    },
    {
      "path": "./public/component---src-pages-produits-*.js",
      "maxSize": "10KB"
    },
    {
      "path": "./public/component---src-pages-index-*.js",
      "maxSize": "25KB"
    },
    {
      "path": "./public/component---src-templates-gallery-page-*.js",
      "maxSize": "25KB"
    },
    {
      "path": "./public/styles.*.css",
      "maxSize": "70KB"
    }
  ]
}
