    "eslint": "9.20.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.3",
    "eslint-plugin-react": "7.37.4",
    "globals": "15.15.0",
    "prettier": "3.5.2"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "serve": "gatsby serve",
    "bundlewatch": "bundlewatch",
    "develop": "gatsby develop",
    "format": "prettier --write .",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "lint:tsc": "tsc"
  },
  "renovate": {
    "extends": [
      "github>browniebroke/renovate-configs:js-app"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "bundlewatch": [
    {
      "path": "./public/app-*.js",
      "maxSize": "150KB"
    },
    {
      "path": "./public/component---src-pages-404-*.js",
      "maxSize": "500B"
    },
    {
      "path": "./public/component---src-pages-contact-*.js",
      "maxSize": "1KB"
    },
    {
      "path": "./public/component---src-pages-produits-*.js",
      "maxSize": "10KB"
    },
    {
      "path": "./public/component---src-pages-index-*.js",
      "maxSize": "25KB"
    },
    {
      "path": "./public/component---src-templates-gallery-page-*.js",
      "maxSize": "5KB"
    },
    {
      "path": "./public/styles.*.css",
      "maxSize": "100KB"
    }
  ]
}
